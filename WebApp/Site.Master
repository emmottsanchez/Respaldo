                    <td style="width:20px" ></td>
                    <td>
                        <asp:Label ID="Label1" runat="server" Font-Size="25px">CCAF Los Héroes</asp:Label><br />
                        <asp:Label ID="Label2" runat="server" Font-Size="15px">Panel Experiencia Comercial</asp:Label>                
                    </td>                    
                    <td style="width:235px; text-align:right; text-align:right" >
                        <asp:Label runat="server" ID="lblPeriodo" Text="Período" Visible="false" Font-Size="15px" />
