                        <img src="../Imagenes/Expertin100x100.png" width="78px" height="78px" style="vertical-align: bottom;" />
                    </td>                                                         
                </tr>            
            </table>

        </div>
        
    </asp:Panel>
        
    <asp:Panel runat="server" ID="Menu" defaultbutton="btnIniciarSesion" style="margin-left:10px">
        <asp:Table runat="server" ID="tbUsuario" Width="100%">
            <asp:TableRow Height="40px">                
                <asp:TableCell HorizontalAlign="Left" ColumnSpan="2">
                    <asp:Panel runat="server" ID="IniciarSesion">
                        Email&nbsp;&nbsp;
                        <asp:TextBox ID="tbEmail" runat="server" CssClass="txtEstandar" Width="250px"></asp:TextBox>
                        <asp:CheckBox ID="cbGuardarEmail" runat="server" Text="Guardar" Checked="True" />&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;

                        Contraseña&nbsp;&nbsp;                                                
                        <asp:TextBox ID="tbContraseña" runat="server" CssClass="txtEstandar" TextMode="Password" Width="100px"></asp:TextBox>&nbsp;&nbsp;
                        <asp:Button ID="btnIniciarSesion" runat="server" cssClass="btnEstandar" Height="20px" Text="Ingresar" />
                        &nbsp;&nbsp;
                        <asp:Label ID="lblMsg0" runat="server" ForeColor="Red" Font-Size="11px"  Font-Bold="true"></asp:Label>
                    </asp:Panel>
                </asp:TableCell>
            </asp:TableRow>

            <asp:TableRow Visible="false">
                <asp:TableCell>
                    <asp:Button ID="btnPrincipal" runat="server" CssClass="BotonMenu" Text="Inicio" />
                    <asp:Button ID="btnIndicadores" runat="server" CssClass="BotonMenu" Text="Tablero Comercial" Width="155px" />                    
                    <asp:Button ID="btnAfiliacionesCanal" runat="server" CssClass="BotonMenu" Text="Afiliaciones por Canal" Width="155px" />
