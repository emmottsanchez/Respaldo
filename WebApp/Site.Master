    <script>
        function MostrarMensaje() {
            var selectedEffect = 'slide';
            var options = {};
            var msDesplegar = 500;
            var msMantener = 3000;
            var msOcultar = 500;

            $("#Mensaje1").show(selectedEffect, options, msDesplegar);

            setTimeout(function () {
                $("#Mensaje1").hide(selectedEffect, options, msOcultar);
            }, msMantener);
        };

        $(function () {
            $(".datepickered").datepicker();
        });
      </script>

    <title>Experiencia Comercial</title>

    <asp:ContentPlaceHolder ID="cphHeader" runat="server" />

</head>
<body>
    <form id="form1" runat="server">
        
    <asp:Panel runat="server" ID="panEncabezado">
        
