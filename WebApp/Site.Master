                        <asp:Label ID="lblUsuarioSesion" runat="server" Text="(Nombre Usuario)" CssClass="lblEstandar"></asp:Label>&nbsp;&nbsp;                        
                        <asp:Button ID="btnCerrarSesion" runat="server" Text="Cerrar Sesión" cssClass="btnEstandar" width="80px" Height="20px" style="margin-right:60px" />
                    </asp:Panel>
